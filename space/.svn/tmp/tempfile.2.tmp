	<script type="text/template" id='tpl_firstACT'>
		<div class='g-activity'>
			<# var activityStatus;#>
			<# if( data.activityStatus == 1){activityStatus="我要报名"}else if( data.activityStatus == 3){activityStatus="进行中"}else if( data.activityStatus == 4){activityStatus="活动已满额"}if( data.activityStatus == 5){activityStatus="已结束"}#>
			<a target='_blank' href='/webrexco/events/detail.htm?id=<#= data.id #>' class='u-activityPic' style='background-image:url(<#= data.posterUrl#>);'></a>
			<div class='f-fl'>
				<a href='/webrexco/events/detail.htm?id=<#= data.id #>' target='_blank' class='f-tof' style='width: 768px;'><#= data.activityName #></a>
				<p><#= data.activityStartTime.substr(0,19) #> ～ <#= data.activityEndTime.substr(0,19) #></p>
				<p class='f-tof' style='width: 768px;'>(<#= data.activityProvince #><#= data.activityCity #>)<#= data.activityAddress #></p>
				<h5>限额<#= data.partiNum #>人</h5>
				<div class='u-selectAndApply clearfix'>
					<div class='g-select'><div class='u-select'></div><span class='u-collectNum'><#= data.collectCount #></span></div>
					<a href='/webrexco/events/detail.htm?id=<#= data.id #>' target='_blank' class='u-apply'><#= activityStatus #></a>
				</div>
			</div>
		</div>
	</script>
	<script type="text/javascript">
		$(document).ready(function(){
			$('.login_btn').click(function(e){
				e.preventDefault();
				checkIsLogin();
			})
			messageStatus();
			getFirstActivity();
			$(".u-activeman").hover(function(){
				var info = $(this).find(".u-activemanInfo");
				if(info.is(":animated"))return;
				info.animate({"bottom":0},200);
			},function(){
				var info = $(this).find(".u-activemanInfo");
				info.animate({"bottom":-76},200);
			});
			$(".J_sendMes").click(function(e){
				e.preventDefault();
				if($(this).hasClass("disable")){
					return;
				}
				var message = $(".J_mesDetail").val().replace(/</g,"&lt;").replace(/>/g,"&gt;");//替代
				if(message==""||message==undefined){
					util.tipsModal(2,"请输入您的投送信息")
					return;
				}
				if(message.length>250){
					util.tipsModal(2,"您的消息长度超出限制");
					return;
				}
				baseModel.post({
					url:"data/leaveMessage.cgi",
					data:{message:message},
					success:function(rslt){
						if(rslt.code==401){
							showIframe("/webrexco/activity/login/login.htm");
							return;
						}else if(rslt.code!=0){
							util.tipsModal(2,rslt.message);
						}else{
							$(".J_mesDetail").val("");
							$(".J_sendMes").addClass("disable");
							$(".u-tipMes").html('亲，您已投送成功，请前往<a href="/webrexco/uc/account.htm" style="font-size:16px;color:#e4393c;">个人中心</a>查看');
							util.tipsModal(2,rslt.message);
						}

					}
				});
			});
			inOrderFadein();
			var flag = true;
			$(window).scroll(function(){
				if(flag == false){
					return;
				}
				if($("body").scrollTop()>600||$("html").scrollTop()>600){
					flag = false;
					$(".u-projectDia").animate({"opacity":1},1000,function(){
						$('.u-projectBos').animate({"opacity":1},1000);

					})
				}
			})
			$(".u-joinNow.J_special").click(function(e){
				e.preventDefault();
				baseModel.get({
					url : "/webrexco/news/data/checkIsLogin.cgi",
					success : function(rslt){
						if(rslt.code==401){
							showIframe("/webrexco/activity/login/login.htm");
						}else if(rslt.ventureStatus == 2&&rslt.ventureLevel == 1){
							location.href = '/webrexco/space/charge.htm';
						}else if(rslt.ventureStatus == 2){
							util.tipsModal(2,"亲，您提交的入驻星空会申请正在审核中，请耐心等待");
						}else if(rslt.ventureStatus == 3){
							util.tipsModal(2,"亲，您申请的星空会审核失败，请联系客服：400-600-6565");
						}else if(rslt.ventureLevel == 2){
							util.tipsModal(2,"您已经是星空会黄金会员！");
						}else if(rslt.ventureLevel == 0||rslt.ventureLevel == null){
							location.href='/webrexco/space/apply.htm';
						}else if(rslt.ventureLevel == 1){
							location.href='/webrexco/space/charge.htm';
						}
					}
				})
			});
		});
		function checkIsLogin(linkUrl){
			if(linkUrl == "/webrexco/register.htm"){
				location.href=linkUrl;
				return;
			}
			baseModel.get({
				url : "/webrexco/news/data/checkIsLogin.cgi",
				success : function(rslt){
					if(rslt.code==401){
						showIframe("/webrexco/activity/login/login.htm");
					}else if(rslt.ventureStatus == 2&&rslt.ventureLevel == 1){
						location.href = '/webrexco/space/charge.htm';
					}else if(rslt.ventureStatus == 2){
						util.tipsModal(2,"亲，您提交的入驻星空会申请正在审核中，请耐心等待");
					}else if(rslt.ventureStatus == 3){
						util.tipsModal(2,"亲，您申请的星空会审核失败，请联系客服：400-600-6565");
					}else if(rslt.ventureLevel == 2){
						util.tipsModal(2,"您已经是星空会黄金会员！");
					}else if(rslt.ventureLevel == 0||rslt.ventureLevel == null){
						location.href='/webrexco/space/apply.htm';
					}else if(rslt.ventureLevel == 1){
						location.href='/webrexco/space/charge.htm';
					}
				}
			})
		}
		function inOrderFadein(){
			$(".u-bannerTxt1").fadeIn(800);
			setTimeout(function(){$(".u-bannerTxt2").fadeIn(800);},800)
			setTimeout(function(){$(".m-sendIpt").fadeIn(800);},1600)
			setTimeout(function(){$(".u-tipMes").fadeIn(800);},2400)
			setTimeout(function(){$(".u-joinUs").fadeIn(800);$(".u-newComer").fadeIn(800);},3200);
		}
		function messageStatus(){
			baseModel.get({
				url : "data/messageStatus.cgi",
				success : function(rslt){
					if(rslt.code!=0){
						return;
					}
					var tipsMes = $(".u-tipMes");
					var status1 = "亲，您已投送成功，请前往"+"<a href='/webrexco/uc/account.htm' style='font-size:16px;color:#e4393c;'>个人中心</a>"+"查看";
					if(rslt.data.status == 0){//未投送
						tipsMes.html("此次投送将消息随机发送至2个boss，回复率99%，建立高价值连接");
					}else{ //已投送
						tipsMes.html(status1);
						$('.J_sendMes').addClass("disable");
					}
				}
			})
		}
		function getFirstActivity(){
			var data = {
				currentPage : 1,
				pageSize : 1
			}
			baseModel.get({
				url : "data/activitySearch.cgi",
				data : data,
				success : function(rslt){
					if(rslt.code!=0){
						return;
					}
					
					$(".g-part5 .g-container").append(baseView.render("#tpl_firstACT",rslt.dataList[0]));
					if(rslt.isCollect){
						$(".u-select").addClass('active');
					}else{
						$(".u-select").removeClass('active');
					}
					$(".u-select").click(function(){
						var _that = $(this);
						baseModel.post({
							url : "/webrexco/events/like.cgi",
							data :{type : 0 ,activityId : rslt.dataList[0].id},
							success : function(rslt){
								if(rslt.code==401){
									showIframe("/webrexco/activity/login/login.htm");
									return;
								}
								if(rslt.code!=0){
									alert(rslt.message);
									return
								}
								if($(".u-select").hasClass('active')){
									_that.removeClass("active");
									$(".u-collectNum").html(parseInt($(".u-collectNum").html())-1);
								}else{
									_that.addClass("active");
									$(".u-collectNum").html(parseInt($(".u-collectNum").html())+1);
								}
							}
						})
					})
				}
			})
		}
	</script>
</body>
</html>
