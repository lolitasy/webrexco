.label {
  display: inline-block;
  vertical-align: top;
  width: 100px;
  margin-right: 25px;
  line-height: 40px;
  font-weight: bolder;
  font-size: 18px;
  color: #666666;
  text-align: right;
}
.wide .label {
  width: 150px;
}
label.wan {
  position: absolute;
  top: 0;
  right: 20px;
  bottom: 0;
  line-height: 40px;
  font-size: 18px;
}
input {
  color: #333333;
}
input[type="text"] {
  box-sizing: border-box;
  width: 410px;
  height: 40px;
  line-height: 40px;
  padding: 0 20px;
  font-family: DengXian, sans-serif;
  font-size: 16px;
  outline: none;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
}
input[type="text"]:focus {
  outline: none;
  border: 1px solid #c8a676;
}
input[type="text"].wan {
  padding-right: 80px;
}
select {
  display: none;
}
.checkbox {
  display: none;
  width: 0;
  height: 0;
  display: inline\0/;
}
.checkbox + label {
  display: inline-block;
  margin-right: 45px;
  height: 40px;
  font-size: 18px;
  color: #666666;
  line-height: 40px;
  padding-left: 20px;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 15px 15px;
  background-position: left center;
}
.checkbox + label.checked {
  background-image: url("data:image/png;base64,");
}
.button {
  box-sizing: border-box;
  margin-top: 55px;
  margin-right: 25px;
  height: 35px;
  line-height: 35px;
  display: inline-block;
  vertical-align: top;
  padding: 0 20px;
  border: none;
  border-radius: 3px;
  background-color: #c8a676;
  font-size: 18px;
  color: #FFFFFF;
  text-align: center;
  cursor: pointer;
  text-decoration: none;
  user-select: none;
}
.button.white {
  background-color: #FFFFFF;
  border: 1px solid #c8a676;
  color: #666666;
}
.main {
  width: 850px;
  display: inline-block;
  vertical-align: top;
}
.main .detail {
  min-height: 200px;
}
.side {
  box-sizing: border-box;
  display: inline-block;
  vertical-align: top;
  width: 290px;
  margin-left: 60px;
  background-color: #F5F5F5;
  border-top: 3px solid #c8a676;
  padding: 8px 20px;
}
.side .header {
  height: 53px;
  line-height: 53px;
  color: #201747;
  font-size: 18px;
  text-align: center;
}
.side .header.border {
  border-bottom: 1px solid #949494;
}
.button {
  height: 42px;
  line-height: 42px;
  padding: 0 45px;
}
.button.red {
  display: inline-block;
  margin-top: 10px;
  height: 30px;
  line-height: 30px;
  padding: 0 15px;
  font-size: 14px;
  background-color: #F63440;
  color: #FFFFFF;
  text-align: center;
  border-radius: 3px;
  cursor: default;
}
.button.red#sign {
  cursor: pointer;
}
#map {
  margin: 16px 0;
  height: 150px;
