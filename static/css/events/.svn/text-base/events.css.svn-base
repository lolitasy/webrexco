  width: 30px;
  height: 30px;
  background-image: url("data:image/png;base64,");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 30px 30px;
  position: absolute;
  top: 0;
  right: 0;
  width: 95px;
  height: 95px;
  cursor: pointer;
}
.mask .popup .input {
  margin-top: 30px;
  padding-left: 100px;
}
