.button {
  box-sizing: border-box;
  margin-top: 55px;
  margin-right: 25px;
  height: 35px;
  line-height: 35px;
  display: inline-block;
  vertical-align: top;
  padding: 0 20px;
  border: none;
  border-radius: 3px;
  background-color: #c8a676;
  font-size: 18px;
  color: #FFFFFF;
  text-align: center;
  cursor: pointer;
  text-decoration: none;
  user-select: none;
}
.button.white {
  background-color: #FFFFFF;
  border: 1px solid #c8a676;
  color: #666666;
}
body {
  background-color: #FFFFFF;
}
.content {
  margin-top: 32px;
}
.m-leftNav {
  float: none;
  display: inline-block;
  vertical-align: top;
}
.list {
  box-sizing: border-box;
  width: 940px;
  padding-top: 14px;
  padding-left: 51px;
  display: inline-block;
  vertical-align: top;
}
.title {
  width: 100%;
  height: 49px;
  line-height: 44px;
  border-bottom: 1px solid #e0e0e0;
}
.title p {
  display: inline-block;
  margin-left: -1px;
  height: 45px;
  color: #000;
  font-size: 20px;
  border-bottom: 4px solid #c8a676;
}
.i-empty {
  display: block;
  margin: 100px auto 0;
  width: 203px;
  height: 179px;
  background-image: url("data:image/png;base64,");
  background-size: 203px 179px;
}
.empty {
  margin-top: 40px;
  text-align: center;
  font-size: 20px;
}
.article {
  font-size: 0;
  margin-top: 25px;
  padding-bottom: 15px;
  border-bottom: 1px solid #c8a676;
}
.poster {
  display: inline-block;
  vertical-align: top;
  margin-right: 20px;
  margin-bottom: 10px;
  width: 290px;
  height: 180px;
  border-radius: 4px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  overflow: hidden;
}
.video {
  width: 290px;
  height: 180px;
  background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAIAAAACCAYAAABytg0kAAAAFElEQVQImWNkYGBoYGBgYGBigAIABzYAhFus9pQAAAAASUVORK5CYII=");
}
.video .play {
  width: 290px;
  height: 180px;
  background-image: url("data:image/png;base64,");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 54px 50px;
}
video {
  width: 290px;
  height: 180px;
}
.right {
  position: relative;
  display: inline-block;
  vertical-align: top;
  width: 100%;
  height: 140px;
}
.right.type-1 {
  width: 570px;
  height: 180px;
}
.right.type-1 .name {
  width: 430px;
}
.right.type-1 .info {
  bottom: 5px;
}
.right.type-3 {
  height: 250px;
}
.right .name {
  display: block;
  width: 750px;
  line-height: 35px;
  color: #333333;
  font-size: 20px;
  text-decoration: none;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.right .button {
  position: absolute;
  top: 0;
  right: 17px;
  margin: 0;
  color: #B39161;
}
.right .detail {
  margin-top: 10px;
  color: #333333;
  font-size: 14px;
}
.right .images {
  margin-top: 20px;
}
.right .image {
  display: inline-block;
  vertical-align: top;
  margin-right: 25px;
  width: 270px;
  height: 170px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  border-radius: 4px;
}
.right .info {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  color: #333333;
  font-size: 12px;
}
.right .info span {
  margin-right: 15px;
}
.pager {
  margin: 30px 0;
  text-align: center;
}
