.button {
  cursor: pointer;
  text-decoration: none;
  user-select: none;
}
.background {
  line-height: normal;
  width: 100%;
  min-width: 1200px;
  height: 600px;
  background-image: url("/webrexco/static/img/ideal/background.jpg");
  background-position: center top;
  background-repeat: no-repeat;
  background-size: 1920px 600px;
}
.background .header {
  box-sizing: border-box;
  position: relative;
  width: 1200px;
  height: 600px;
  margin: 0 auto;
  padding: 220px 0 0 280px;
  font-weight: lighter;
  color: #1F1F1F;
  user-select: none;
  cursor: default;
}
.background .header .title {
  display: inline-block;
  vertical-align: middle;
  font-size: 72px;
}
.background .header .point {
  display: inline-block;
  vertical-align: middle;
  font-size: 72px;
  margin: 0 10px;
}
.background .header .subtitle {
  display: inline-block;
  vertical-align: middle;
  font-size: 40px;
}
.background .header .buttons {
  margin-top: 10px;
  margin-left: 95px;
}
.background .header .button {
  display: inline-block;
  height: 40px;
  line-height: 40px;
  margin-right: 30px;
  padding: 0 10px;
  font-size: 22px;
  font-weight: normal;
  color: #1F1F1F;
  border: 1px solid #004D43;
  border-radius: 5px;
}
.content {
  width: 1200px;
  min-height: 100%;
  margin: 0 auto;
  background-color: #FFFFFF;
}
.content .title {
  margin: 50px 0;
  font-size: 30px;
  font-weight: bold;
  text-align: center;
  color: #201747;
}
.content .swiper-slide {
  position: relative;
  height: 320px;
}
.content .swiper-slide .img {
  width: 235px;
  height: 286px;
}
.content .swiper-slide .name {
  position: absolute;
  width: 100%;
  bottom: 34px;
  height: 34px;
  line-height: 34px;
  background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAIAAAACCAYAAABytg0kAAAAFElEQVQImWNkYGBoYGBgYGBigAIABzYAhFus9pQAAAAASUVORK5CYII=");
  font-size: 18px;
  text-align: center;
  color: #FFFFFF;
}
.content .swiper-slide .button {
  position: absolute;
  width: 100%;
  bottom: 0;
  height: 34px;
  line-height: 34px;
  background-color: #c8a676;
  font-size: 18px;
  text-align: center;
  color: #FFFFFF;
}
.content .container {
  position: relative;
  margin: 0 -50px;
  padding: 0 50px;
}
.content .swiper-button-prev {
  margin-top: 25px;
  background-image: url("data:image/png;base64,");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 23px 40px;
}
.content .swiper-button-prev:hover {
  background-image: url("data:image/png;base64,");
}
.content .swiper-button-next {
  margin-top: 25px;
  background-image: url("data:image/png;base64,");
  background-position: center;
  background-repeat: no-repeat;
  background-size: 23px 40px;
}
.content .swiper-button-next:hover {
  background-image: url("data:image/png;base64,");
}
.content .block {
  font-size: 0;
  background-color: #F5F5F5;
  margin-bottom: 30px;
}
.content .block .half {
  position: relative;
  display: inline-block;
  vertical-align: top;
  width: 600px;
  height: 300px;
}
.content .block .image {
  width: 600px;
  height: 300px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}
.content .block .name {
  margin-top: 36px;
  font-size: 36px;
  font-weight: bold;
  text-align: center;
  color: #c8a676;
}
.content .block .description {
  width: 325px;
  margin: 30px auto;
  line-height: 34px;
  font-size: 18px;
  text-align: center;
  color: #333333;
  white-space: pre-wrap;
}
.content .block .buttons {
  position: absolute;
  width: 100%;
  bottom: 30px;
  text-align: center;
}
.content .block .button {
  display: inline-block;
  width: 175px;
  height: 40px;
  line-height: 40px;
  background-color: #c8a676;
  font-size: 18px;
  text-align: center;
  color: #FFFFFF;
  border-radius: 5px;
  margin: 0 20px;
}
.link {
  margin-bottom: 100px;
  height: 600px;
  background-image: url("/webrexco/static/img/ideal/link.png");
  background-repeat: no-repeat;
